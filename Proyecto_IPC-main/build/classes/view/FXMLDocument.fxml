<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="633.0" prefWidth="904.0" spacing="10.0" stylesheets="@../resources/estilos.css" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="poiupv.FXMLDocumentController">
   <children>
      <HBox prefHeight="36.0" prefWidth="744.0">
         <children>
            <Label fx:id="questionNumber" text="Label" />
         </children>
      </HBox>
      <VBox prefHeight="200.0" prefWidth="100.0" spacing="10.0">
         <children>
            <Label fx:id="questionLabel" text="Label">
               <font>
                  <Font size="25.0" />
               </font>
            </Label>
            <VBox prefHeight="200.0" prefWidth="100.0" spacing="10.0">
               <children>
                  <RadioButton fx:id="buttonA" mnemonicParsing="false" onAction="#handleButtonA" text="RadioButton" />
                  <RadioButton fx:id="buttonB" mnemonicParsing="false" onAction="#handleButtonB" text="RadioButton" />
                  <RadioButton fx:id="buttonC" mnemonicParsing="false" onAction="#handleButtonC" text="RadioButton" />
                  <RadioButton fx:id="buttonD" mnemonicParsing="false" onAction="#handleButtonC" text="RadioButton" />
               </children>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </VBox>
         </children>
         <VBox.margin>
            <Insets left="20.0" />
         </VBox.margin>
      </VBox>
      <Pane prefHeight="313.0" prefWidth="604.0">
         <children>
            <ScrollPane fx:id="map_scrollpane" hbarPolicy="NEVER" onMouseMoved="#muestraPosicion" pannable="true" prefHeight="322.0" prefWidth="744.0" vbarPolicy="NEVER">
               <content>
                  <Pane>
                     <children>
                        <ImageView fx:id="map" layoutX="-100.0" layoutY="-119.0" onMousePressed="#mapMousePress" onMouseReleased="#mapMouseRelease" pickOnBounds="true">
                           <image>
                              <Image url="@../resources/carta_nautica.jpg" />
                           </image>
                           <cursor>
                              <Cursor fx:constant="DEFAULT" />
                           </cursor>
                        </ImageView>
                     </children>
                  </Pane>
               </content>
               <cursor>
                  <Cursor fx:constant="DEFAULT" />
               </cursor>
            </ScrollPane>
            <VBox layoutX="691.0" layoutY="230.0" prefHeight="92.0" prefWidth="53.0" spacing="5.0">
               <children>
                  <Button mnemonicParsing="false" text="Button" />
                  <Button mnemonicParsing="false" text="Button" />
                  <Button mnemonicParsing="false" text="Button" />
               </children>
            </VBox>
         </children>
      </Pane>
      <HBox alignment="CENTER_LEFT" spacing="15.0">
         <VBox.margin>
            <Insets bottom="12.0" right="12.0" top="20.0" />
         </VBox.margin>
         <children>
            <Button fx:id="cancelButton" mnemonicParsing="false" prefHeight="30.0" prefWidth="60.0" text="Button" />
            <Button fx:id="nextButton" mnemonicParsing="false" prefHeight="30.0" prefWidth="60.0" text="Button" />
         </children>
      </HBox>
   </children>
   <padding>
      <Insets left="80.0" right="80.0" top="30.0" />
   </padding>
</VBox>
