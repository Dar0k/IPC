<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.ToggleGroup?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="438.0" prefWidth="758.0" spacing="10.0" stylesheets="@../resources/estilos.css" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="poiupv.FXMLDocumentController">
   <children>
      <HBox prefHeight="36.0" prefWidth="744.0">
         <children>
            <Label fx:id="questionNumber" text="Label" />
         </children>
      </HBox>
      <VBox prefHeight="200.0" prefWidth="100.0" spacing="10.0">
         <children>
            <Label fx:id="questionLabel" text="Label">
               <font>
                  <Font size="25.0" />
               </font>
            </Label>
            <VBox fx:id="vboxRadio" prefHeight="200.0" prefWidth="100.0" spacing="10.0">
               <children>
                  <RadioButton fx:id="radioButtonA" mnemonicParsing="false" text="RadioButton">
                     <toggleGroup>
                        <ToggleGroup fx:id="rad" />
                     </toggleGroup></RadioButton>
                  <RadioButton fx:id="radioButtonB" mnemonicParsing="false" text="RadioButton" toggleGroup="$rad" />
                  <RadioButton fx:id="radioButtonC" mnemonicParsing="false" text="RadioButton" toggleGroup="$rad" />
                  <RadioButton fx:id="radioButtonD" mnemonicParsing="false" text="RadioButton" toggleGroup="$rad" />
               </children>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </VBox>
         </children>
         <VBox.margin>
            <Insets left="20.0" />
         </VBox.margin>
      </VBox>
      <Pane prefHeight="313.0" prefWidth="604.0">
         <children>
            <ScrollPane fx:id="map_scrollpane" hbarPolicy="NEVER" onMouseMoved="#muestraPosicion" pannable="true" prefHeight="160.0" prefWidth="598.0" vbarPolicy="NEVER">
               <content>
                  <ImageView fx:id="map" onMousePressed="#mapMousePress" onMouseReleased="#mapMouseRelease" pickOnBounds="true">
                     <image>
                        <Image url="@../resources/carta_nautica.jpg" />
                     </image>
                     <cursor>
                        <Cursor fx:constant="DEFAULT" />
                     </cursor>
                  </ImageView>
               </content>
               <cursor>
                  <Cursor fx:constant="DEFAULT" />
               </cursor>
            </ScrollPane>
         </children>
      </Pane>
      <HBox alignment="CENTER_LEFT" spacing="15.0">
         <VBox.margin>
            <Insets bottom="12.0" right="12.0" top="20.0" />
         </VBox.margin>
         <children>
            <Button fx:id="goBackButton" mnemonicParsing="false" onAction="#handleGoBackButton" prefHeight="30.0" prefWidth="60.0" text="Go Back" />
            <Button fx:id="sendButton" mnemonicParsing="false" onAction="#handleSendButton" prefHeight="30.0" prefWidth="60.0" text="Send" />
         </children>
      </HBox>
   </children>
   <padding>
      <Insets left="80.0" right="80.0" top="30.0" />
   </padding>
</VBox>
