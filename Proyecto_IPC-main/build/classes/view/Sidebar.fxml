<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox fx:id="sb" alignment="CENTER" fillWidth="false" maxWidth="283.0" minHeight="320.0" minWidth="125.0" prefHeight="441.0" prefWidth="136.0" spacing="25.0" style="-fx-background-color: rgba(255,255,255,0.7);" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="poiupv.SidebarController">
    <children>
        <Button fx:id="testButton" mnemonicParsing="false" onAction="#handleTest" onMouseEntered="#OnMouseHoverEnter" onMouseExited="#OnMouseHoverExited" style="-fx-background-color: transparent; -fx-background-radius: 50;" text="TEST">
            <font>
                <Font size="15.0" />
            </font>
            <cursor>
                <Cursor fx:constant="HAND" />
            </cursor>
        </Button>
        <Button fx:id="resultsButton" mnemonicParsing="false" onAction="#handleResult" onMouseEntered="#OnMouseHoverEnter" onMouseExited="#OnMouseHoverExited" prefWidth="105.0" style="-fx-background-color: transparent; -fx-background-radius: 50;" text="RESULTS">
            <font>
                <Font size="15.0" />
            </font>
            <cursor>
                <Cursor fx:constant="HAND" />
            </cursor>
        </Button>
        <Button fx:id="profileButton" mnemonicParsing="false" onAction="#handleProfile" onMouseEntered="#OnMouseHoverEnter" onMouseExited="#OnMouseHoverExited" prefWidth="105.0" style="-fx-background-color: transparent; -fx-background-radius: 50;" text="PROFILE">
            <font>
                <Font size="15.0" />
            </font>
            <cursor>
                <Cursor fx:constant="HAND" />
            </cursor>
        </Button>
        <Button fx:id="logOutButton" mnemonicParsing="false" onAction="#handleLogOut" onMouseEntered="#OnMouseHoverEnter" onMouseExited="#OnMouseHoverExited" prefWidth="105.0" style="-fx-background-color: transparent; -fx-background-radius: 50;" text="LOG OUT">
            <font>
                <Font size="15.0" />
            </font>
            <cursor>
                <Cursor fx:constant="HAND" />
            </cursor>
        </Button>
    </children>
    <padding>
        <Insets bottom="25.0" left="10.0" right="10.0" top="25.0" />
    </padding>
</VBox>
